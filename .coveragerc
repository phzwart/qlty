[run]
source = qlty
omit = 
    */tests/*
    */test_*
    */__pycache__/*
    */site-packages/*
    */torch/*
    qlty/__init__.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

